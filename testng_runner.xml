<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Demoblaze Automation" verbose="1">
    <test name="Demoblaze Smoke Test">
        <classes>
            <class name="com.Adidas.TestNGTests.LoginPage">
                <methods>
                    <include name="loginWithInvalidPassword"></include>
                </methods>
            </class>
            <class name="com.Adidas.TestNGTests.LoginPageDDT">
                <methods>
                <exclude name="notLoginWithoutClicking"></exclude>
            </methods>
            </class>
        </classes>
    </test>
</suite>
